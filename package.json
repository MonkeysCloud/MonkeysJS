{
  "name": "monkeysjs",
  "version": "1.0.0",
  "description": "A lightweight, reactive JavaScript library with built-in HTTP client, declarative DOM binding, caching, WebSockets, and more",
  "main": "dist/monkeysjs.cjs",
  "module": "dist/monkeysjs.esm.js",
  "browser": "dist/monkeysjs.min.js",
  "unpkg": "dist/monkeysjs.min.js",
  "jsdelivr": "dist/monkeysjs.min.js",
  "types": "dist/monkeysjs.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/monkeysjs.d.ts",
      "import": "./dist/monkeysjs.esm.js",
      "require": "./dist/monkeysjs.cjs",
      "browser": "./dist/monkeysjs.min.js",
      "default": "./dist/monkeysjs.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run build:types && npm run build:cjs",
    "build:cjs": "node scripts/build-cjs.mjs",
    "build:types": "tsc --emitDeclarationOnly --declaration --outDir dist",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "reactive",
    "javascript",
    "framework",
    "ajax",
    "fetch",
    "http-client",
    "dom-binding",
    "declarative",
    "lightweight",
    "websocket",
    "caching",
    "monkeyscloud"
  ],
  "author": "Jorge Peraza <jorge@monkeys.cloud>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkeyscloud/monkeysjs.git"
  },
  "bugs": {
    "url": "https://github.com/monkeyscloud/monkeysjs/issues"
  },
  "homepage": "https://monkeyscloud.com/monkeysjs",
  "devDependencies": {
    "@types/node": "^25.0.9",
    "@vitest/coverage-v8": "^1.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.56.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
